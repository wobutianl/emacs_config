;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;;;; key binding ;;;;;;;;;;;;;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

(global-set-key [f1] 'other-window)
;; 跳转到 Emacs 的另一个buffer窗口

(global-set-key [f2] 'undo)
(global-set-key [f3] 'redo)

;;set the F4 to kill current buffer
(global-set-key [f4] 'kill-this-buffer) 

(global-set-key [f9] 'speedbar)
;;设置f9调用speedbar命令
